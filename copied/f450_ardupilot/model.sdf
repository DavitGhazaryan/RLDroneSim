<?xml version='1.0'?>
<sdf version="1.9">
  <model name="f450_with_ardupilot">
    <include>
      <uri>model://f450</uri>
    </include>

    <!-- Lift-Drag Plugin for Rotor 0 (Front Right - CCW) -->
    <plugin filename="gz-sim-lift-drag-system"
        name="gz::sim::systems::LiftDrag">
      <a0>0.3</a0>
      <alpha_stall>1.4</alpha_stall>
      <cla>4.2500</cla>
      <cda>0.10</cda>
      <cma>0.0</cma>
      <cla_stall>-0.025</cla_stall>
      <cda_stall>0.0</cda_stall>
      <cma_stall>0.0</cma_stall>
      <area>0.0052</area> <!-- Larger area for F450 props (10 inch) -->
      <air_density>1.2041</air_density>
      <cp>0.084 0 0</cp>
      <forward>0 1 0</forward>
      <upward>0 0 1</upward>
      <link_name>f450::rotor_0</link_name>
    </plugin>
    <plugin filename="gz-sim-lift-drag-system"
